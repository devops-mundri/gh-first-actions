name: CI/CD Pipeline=
on:
  push:
    branches:
      - master
jobs:
  build:
    runs-on: ubuntu-latest
    steps:
      - name: Checkout code
        uses: actions/checkout@v2
      - name: Install dependencies
        run: npm install
      - name: Lint code
        run: npm run lint
      - name: Test code
        run: npm test
      - name:Build code
        run: npm run build

  deploy:
    runs-on: ubuntu-latest
    needs: build
